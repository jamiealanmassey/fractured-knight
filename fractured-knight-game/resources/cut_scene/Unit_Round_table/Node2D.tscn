script/source = "extends Node2D
onready var camera = get_node('../Camera2D')

var time

func _ready():
	var vsize = get_viewport_rect().size
	get_node(\"../CanvasLayer/ColorRect\").set_size(vsize)
	time = 0
	set_process(true)
	pass

func _process(delta):
	time += delta
	if time >= 2:
		if !(check_set_up()):
			set_up()
		else:
			acting()
	pass

func acting():
	for N in self.get_children():
		if N.get_child_count() >0:
			rotating(N)
	pass

func rotating(node):
	var sp = node.get_node('light')
	if (sp.position != Vector2(0,0)):
		node.rotate(0.01)
		if (sp.position.x > node.position.x):
			sp.position.x -=1
		if (sp.position.x < node.position.x):
			sp.position.x +=1
		if (sp.position.y > node.position.y):
			sp.position.y -=1
		if( sp.position.y< node.position.y):
			sp.position.y +=1
	if (sp.position.x > -1) && (sp.position.x < 1):
		sp.position.x = 0
	if  (sp.position.y >-1) && (sp.position.y < 1):
		sp.position.y = 0
	pass

func set_up():
	for N in self.get_children():
		if N.get_child_count() >0:
			var light = N.get_node('light')
			light.visible = true
			light.increase()
	pass

func check_set_up():
	var count = 0
	for N in self.get_children():
		if N.get_child_count() >0:
			if N.get_node('light').fit == true:
				N.get_node('piece').visible = false
				count +=1
	if count == 5:
		return true
	else:
		return false
	return count
	pass	

func is_united():
	var count = 0
	for N in self.get_children():
		if N.get_child_count() >0:
			if(N.get_node('light').is_united()):
				count += 1
	if count == 5:
		return true
	else:
		return false
	pass

func fade():
	var count = 0
	for N in self.get_children():
		if N.get_child_count() >0:
			N.get_node('light').decrease()
	pass"

