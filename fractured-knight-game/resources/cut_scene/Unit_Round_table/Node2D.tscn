script/source = "extends Node2D
onready var camera = get_node('../Camera2D')

var time
var centre = Vector2(0,0)
var set_up_done
var unit_done

func _ready():
	var vsize = get_viewport_rect().size
	get_node(\"../CanvasLayer/ColorRect\").set_size(vsize)
	time =0
	set_up_done = false
	unit_done = false
	set_process(true)
	pass

func _process(delta):
	if !(check_set_up()):
		set_up()
	else:
		acting()
	pass

func acting():
	for N in self.get_children():
		if N.get_child_count() >0:
			rotating(N)
	pass

func rotating(node):
	var sp = node.get_node('light')
	if (sp.position != centre):
		node.rotate(0.01)
	if (sp.position.x > node.position.x):
		sp.position.x -=1
	elif (sp.position.x < node.position.x):
		sp.position.x +=1
	if (sp.position.y > node.position.y):
		sp.position.y -=1
	elif( sp.position.y< node.position.y):
		sp.position.y +=1
	pass

func set_up():
	for N in self.get_children():
		if N.get_child_count() >0:
			N.get_node('light').increase()
	pass

func check_set_up():
	var count = 0
	for N in self.get_children():
		if N.get_child_count() >0:
			if N.get_node('light').fit == true:
				count +=1
	if (count ==5):
		return true
	return false
	pass	

func check_unit():
	var count = 0
	for N in self.get_children():
		if N.get_child_count() >0:
			if N.get_node('light').position == centre:
				count+=1
	if count == 5:
		return true
	return false
	pass"

