	scale = s_size
	pass

func expand():	
	if can_shink == false:
		visible = true
		scale += i_size
		if scale >= m_size:
			can_shink = true
			print(can_shink)
	pass

func shinking():
	if can_shink== true:
		table.visible = true
		scale -= i_size
		if scale <= s_size:
			visible = false
			show_table = true
	pass

func showing_table():
	if show_table == true:
		return true
	else:
		return false
	pass"
