script/source = "extends AnimatedSprite
signal finished_animation


func _ready():
	self.animation = \"Fall\"

func on_hitting_ground():
	if self.animation == \"Fall\":
		animation = \"Hit\"
		connect(\"animation_finished\",self, \"turn_stay\")

func turn_stay():
	animation = \"Stay\"
	emit_signal(\"finished_animation\")

func fly_in():
	var vector = Vector2(0,10)
	translate(vector)
	pass"
