	set_process(true)
	pass

func _process(delta):
	move()
	pass

func move():
	if (sprite.position.y < 0):
		sprite.fly_in()
	if (sprite.position.y >= 0):
		if position.y >= 0:
			sprite.on_hitting_ground()
		else:
			if (position.y >= -2000):
				translate(motion2)
			else:
				translate(motion1)
	pass
